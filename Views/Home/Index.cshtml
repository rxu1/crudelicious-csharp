<div class="container-fluid">
  <div class="row"> 
    <div class="col-md-12">
      <h1 id="header">Welcome to CRUDelicious</h1>
    </div>
  </div>
  <div class="row">
    <div class="col-md-10">
      <h4 id="h4-block">Check out some recent dishes!</h4>
      <hr>
    </div>
    <div class="col-md-2">
      <a href="/new" id="a-link">Add a Dish</a>
    </div>
  </div>
    @{
      foreach (var a in Model) 
      {
        <div>
          <a href="/@a.DishId">@a.Name</a>
          <p>by @a.Chef </p>
        </div>
      }
    }
</div>